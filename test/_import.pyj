class Alpha:
    def __init__(self):
        self.name = 'Alpha'
    def getName(self):
        return self.name

@external
class Bravo:
    @staticmethod
    def setName(obj, name):
        pass

def testBravo():
    b = Bravo()
    assert.equal(b.getName(), "Bravo")
    assert.equal(Bravo.getName(b), "Bravo")
    Bravo.setName(b, "Charlie")
    assert.equal(b.getName(), "Charlie")
